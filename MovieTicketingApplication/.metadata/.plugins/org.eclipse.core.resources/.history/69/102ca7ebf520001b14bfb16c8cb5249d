package com.manipal.bean;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

@Entity
public class Movie {


	@Id
	@GeneratedValue(strategy = GenerationType.AUTO)
	private int movieId;
	private String movieName;
	private String rating;
	private int availableNoOfTickets;

	public int getMovieId() {
		return movieId;
	}

	public void setMovieId(int movieId) {
		this.movieId = movieId;
	}

	public String getMovieName() {
		return movieName;
	}

	public void setMovieName(String movieName) {
		this.movieName = movieName;
	}

	public String getRating() {
		return rating;
	}

	public void setRating(String rating) {
		this.rating = rating;
	}

	public int getAvailableNoOfTickets() {
		return availableNoOfTickets;
	}

	public void setAvailableNoOfTickets(int availableNoOfTickets) {
		this.availableNoOfTickets = availableNoOfTickets;
	}

	public Movie(int movieId, String movieName, String rating, int availableNoOfTickets) {
		super();
		this.movieId = movieId;
		this.movieName = movieName;
		this.rating = rating;
		this.availableNoOfTickets = availableNoOfTickets;
	}

	public Movie() {
	}

}
